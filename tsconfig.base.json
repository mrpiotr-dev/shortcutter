{
  "compilerOptions": {
    "declaration": false,
    "diagnostics": false,
    "downlevelIteration": true,
    "esModuleInterop": true,
    "moduleResolution": "Node",
    "preserveConstEnums": true,
    "module": "ESNext",
    "lib": ["ES2015", "DOM"],
    "removeComments": true,
    "strict": true,
    "sourceMap": true,
    "target": "ES5",
    "typeRoots": [
      "./node_modules/@types",
    ]
  }
}
